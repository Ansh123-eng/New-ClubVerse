q <!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Our Team - Club Verse</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@300;400;500;600;700&family=Montserrat:wght@300;400;500;600;700&family=Playfair+Display:wght@400;500;600;700&display=swap" rel="stylesheet">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" />
  <link rel="stylesheet" href="/styles/dashbar.css">
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    body {
      font-family: 'Montserrat', sans-serif;
      color: #fff;
      background: #0a0a0a;
      overflow-x: hidden;
      position: relative;
    }

    body::before {
      content: '';
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background:
        radial-gradient(circle at 20% 20%, rgba(212, 175, 55, 0.1) 0%, transparent 45%),
        radial-gradient(circle at 80% 80%, rgba(212, 175, 55, 0.1) 0%, transparent 45%);
      z-index: -1;
    }

    .team-container {
      max-width: 1200px;
      margin: 120px auto 80px;
      padding: 0 5%;
      background: linear-gradient(135deg, rgba(255, 255, 255, 0.08) 0%, rgba(255, 255, 255, 0.03) 100%);
      backdrop-filter: blur(25px);
      border: 1px solid rgba(255, 255, 255, 0.15);
      border-radius: 25px;
      padding: 60px;
      position: relative;
      z-index: 2;
      box-shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
    }

    .team-container::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(45deg, transparent, rgba(212, 175, 55, 0.08), transparent);
      border-radius: 25px;
      z-index: -1;
    }

    .team-container::after {
      content: '';
      position: absolute;
      top: -2px;
      left: -2px;
      right: -2px;
      bottom: -2px;
      background: linear-gradient(45deg, #d4af37, #ffd700, #d4af37);
      border-radius: 27px;
      z-index: -2;
      opacity: 0.1;
    }

    .team-header {
      text-align: center;
      margin-bottom: 70px;
      position: relative;
    }

    .team-header h1 {
      font-family: 'Playfair Display', serif;
      font-size: 3.5rem;
      color: #d4af37;
      margin-bottom: 25px;
      position: relative;
      text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
      background: linear-gradient(45deg, #d4af37, #ffd700);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .team-header h1::after {
      content: '';
      position: absolute;
      bottom: -15px;
      left: 50%;
      transform: translateX(-50%);
      width: 120px;
      height: 4px;
      background: linear-gradient(45deg, #d4af37, #ffd700);
      border-radius: 2px;
    }

    .team-header p {
      font-size: 1.2rem;
      color: rgba(255, 255, 255, 0.85);
      line-height: 1.7;
      max-width: 600px;
      margin: 0 auto;
    }

    .team-members {
      display: flex;
      flex-direction: column;
      gap: 60px;
    }

    .team-member {
      display: flex;
      align-items: center;
      gap: 50px;
      background: linear-gradient(135deg, rgba(255, 255, 255, 0.05) 0%, rgba(255, 255, 255, 0.02) 100%);
      border-radius: 20px;
      padding: 40px;
      border: 1px solid rgba(255, 255, 255, 0.1);
      position: relative;
      overflow: hidden;
    }

    .team-member::before {
      content: '';
      position: absolute;
      top: 0;
      bottom: 0;
      width: 4px;
      background: linear-gradient(180deg, #d4af37, #ffd700);
      left: 0;
    }

    .team-member:nth-child(even) {
      flex-direction: row-reverse;
    }

    .team-member:nth-child(even)::before {
      left: auto;
      right: 0;
    }

    .team-image {
      flex-shrink: 0;
      width: 200px;
      height: 200px;
      border-radius: 50%;
      overflow: hidden;
      border: 4px solid #d4af37;
      box-shadow: 0 10px 30px rgba(212, 175, 55, 0.3);
      position: relative;
    }

    .team-image img {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .team-info {
      flex: 1;
    }

    .team-info h3 {
      font-family: 'Playfair Display', serif;
      font-size: 2rem;
      color: #d4af37;
      margin-bottom: 10px;
      font-weight: 600;
    }

    .team-info p {
      font-size: 1.1rem;
      color: rgba(255, 255, 255, 0.9);
      margin-bottom: 20px;
      line-height: 1.6;
    }

    .team-role {
      font-weight: 500;
      color: #ffd700;
      margin-bottom: 15px;
      font-size: 1.2rem;
    }

    .team-social {
      display: flex;
      gap: 15px;
    }

    .team-social a {
      color: #d4af37;
      font-size: 1.5rem;
      padding: 12px;
      border-radius: 50%;
      background: rgba(255, 255, 255, 0.05);
      border: 2px solid rgba(212, 175, 55, 0.3);
      transition: all 0.3s ease;
      display: flex;
      align-items: center;
      justify-content: center;
      width: 50px;
      height: 50px;
    }

    .team-social a:hover {
      color: #ffd700;
      background: rgba(212, 175, 55, 0.1);
      transform: translateY(-2px);
      box-shadow: 0 5px 15px rgba(212, 175, 55, 0.4);
    }

    @media (max-width: 768px) {
      .team-container {
        padding: 40px 20px;
        margin: 100px auto 60px;
      }

      .team-header h1 {
        font-size: 2.8rem;
      }

      .team-members {
        gap: 40px;
      }

      .team-member {
        flex-direction: column;
        text-align: center;
        gap: 30px;
        padding: 30px;
      }

      .team-member:nth-child(even) {
        flex-direction: column;
      }

      .team-member::before {
        display: none;
      }

      .team-image {
        width: 150px;
        height: 150px;
      }

      .team-info h3 {
        font-size: 1.6rem;
      }

      .team-info p {
        font-size: 1rem;
      }
    }

    @media (max-width: 480px) {
      .team-header h1 {
        font-size: 2.5rem;
      }

      .team-container {
        padding: 30px 15px;
      }

      .team-member {
        padding: 25px;
      }
    }
  </style>
</head>
<body>
  <div class="scroll-progress" id="scrollProgress"></div>

  <nav class="nav" id="navbar">
    <div class="nav-content">
      <div class="logo">
        <img src="/images/club-logo-2.png" alt="Club Verse Logo" onerror="this.onerror=null; this.src='data:image/svg+xml,%3Csvg xmlns=&#39;http:www.w3.org/2000/svg&#39; viewBox=&#39;0 0 200 100&#39>%3E%3Ctext x=&#39;100&#39; y=&#39;60&#39; font-family=&#39;serif&#39; font-size=&#39;24&#39; text-anchor=&#39;middle&#39> fill=&#39;%23d4af37&#39;%3EClub Verse%3C/text%3E%3C/svg%3E';">
      </div>
      <div class="menu">
        <ul>
          <li><a href="/api/dashboard">Home</a></li>
          <li><a href="/api/bar">Bars</a></li>
          <li><a href="#events">Events</a></li>
          <li><a href="#contact">Contact</a></li>
          <li><a href="/api/weather" class="weather-btn" style="background: linear-gradient(45deg, #d4af37, #ffd700); color: #000; border-radius: 20px; padding: 8px 18px; font-weight: 600; margin-left: 10px;">Weather</a></li>
        </ul>
      </div>
    </div>
  </nav>

  <div class="team-container">
    <div class="team-header">
      <h1>Our Team</h1>
      <p>Meet the passionate professionals behind Club Verse who make every night unforgettable with their dedication and expertise</p>
    </div>

    <div class="team-members">
      <% team.forEach((member, index) => { %>
        <div class="team-member">
          <div class="team-image">
            <img src="<%= member.image %>" alt="<%= member.name %>">
          </div>
          <div class="team-info">
            <h3><%= member.name %></h3>
            <div class="team-role"><%= member.role %></div>
            <p>Passionate about creating exceptional nightlife experiences and bringing people together in the most vibrant atmosphere.</p>
            <div class="team-social">
              <a href="#"><i class="fab fa-instagram"></i></a>
              <a href="#"><i class="fab fa-github"></i></a>
              <a href="#"><i class="fab fa-linkedin-in"></i></a>
            </div>
          </div>
        </div>
      <% }) %>
    </div>
  </div>

  <footer class="footer" id="contact">
    <div class="footer-content">
      <div class="footer-col">
        <img src="/images/club-logo-2.png" alt="Club Verse Logo" style="max-width: 150px;" onerror="this.onerror=null; this.src='data:image/svg+xml,%3Csvg xmlns=&#39;http:www.w3.org/2000/svg&#39; viewBox=&#39;0 0 200 100&#39>%3E%3Ctext x=&#39;100&#39; y=&#39;60&#39; font-family=&#39;serif&#39; font-size=&#39;24&#39; text-anchor=&#39;middle&#39> fill=&#39;%23d4af37&#39;%3EClub Verse%3C/text%3E%3C/svg%3E';">
        <p style="margin-top: 20px; color: rgba(255,255,255,0.7); line-height: 1.6;">Experience the ultimate nightlife destination where luxury meets entertainment.</p>
      </div>
      <div class="footer-col">
        <h4>About Us</h4>
        <ul>
          <li><a href="/api/ourservices">Our Services</a></li>
          <li><a href="/api/privacy-policy">Privacy Policy</a></li>
          <li><a href="/api/team">Our Team</a></li>
        </ul>
      </div>
      <div class="footer-col">
        <h4>Get Help</h4>
        <ul>
          <li><a href="/api/FAQ">FAQ</a></li>
          <li><a href="/api/contactus">Contact Us</a></li>
        </ul>
      </div>
      <div class="footer-col">
        <h4>Contact Info</h4>
        <ul>
          <li><a href="tel:+911234567890">+91 1234567890</a></li>
          <li><a href="mailto:info@clubverse.com">info@clubverse.com</a></li>
          <li style="color: rgba(255,255,255,0.7);">Sector 17, Chandigarh</li>
        </ul>
        <div class="social-links">
          <a href="#"><i class="fab fa-facebook-f"></i></a>
          <a href="#"><i class="fab fa-instagram"></i></a>
          <a href="#"><i class="fab fa-twitter"></i></a>
          <a href="#"><i class="fab fa-linkedin-in"></i></a>
        </div>
      </div>
    </div>
    <div style="text-align: center; margin-top: 40px; padding-top: 30px; border-top: 1px solid rgba(255,255,255,0.1); color: rgba(255,255,255,0.5);">
      <p>&copy; 2024 Club Verse. All rights reserved. | Designed for the ultimate nightlife experience.</p>
    </div>
  </footer>

  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // Scroll Progress Bar
      const scrollProgress = document.getElementById('scrollProgress');
      function updateScrollProgress() {
        const scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
        const scrollHeight = document.documentElement.scrollHeight - document.documentElement.clientHeight;
        const scrollPercent = (scrollTop / scrollHeight) * 100;
        scrollProgress.style.width = scrollPercent + '%';
      }
      window.addEventListener('scroll', updateScrollProgress);

      // Navbar scroll effect
      const navbar = document.getElementById('navbar');
      window.addEventListener('scroll', () => {
        if (window.scrollY > 100) {
          navbar.classList.add('scrolled');
        } else {
          navbar.classList.remove('scrolled');
        }
      });

      // Smooth scrolling
      document.querySelectorAll('a[href^="#"]').forEach(anchor => {
        anchor.addEventListener('click', function (e) {
          e.preventDefault();
          const target = document.querySelector(this.getAttribute('href'));
          if (target) {
            target.scrollIntoView({
              behavior: 'smooth',
              block: 'start'
            });
          }
        });
      });
    });
  </script>
</body>
</html>
